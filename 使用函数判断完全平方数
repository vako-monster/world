#include <stdio.h>
#include <math.h>

int IsSquare( int n );

int main()
{
    int n;

    scanf("%d", &n);
    if ( IsSquare(n) ) printf("YES\n");
    else printf("NO\n");

    return 0;
}
int IsSquare( int n ){
	int i;
	if(n>0){
	
	for(i=1;i<=n;i++){
		if(n==i*i)
		return 1; 
	}
}
if(n==0)
return 1;
	return 0;
}
