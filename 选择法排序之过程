#include <stdio.h>

int main()
{
    int i,j,a[10],n,max,k;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<n-1;i++){
        max=i;
        for(j=i+1;j<n;j++){
            if(a[j]<a[max]){
                max=j;
            }
        }
        k=a[max];
        a[max]=a[i];
        a[i]=k;
        
    for(j=0;j<n-1;j++){
        printf("%d ",a[j]);
    }
    printf("%d\n",a[n-1]);
    }
    if(n==1){
        printf("%d\n",a[0]);
    }
    return 0;
}
