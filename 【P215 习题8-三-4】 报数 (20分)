#include <stdio.h>
#define MAXN 20

void CountOff( int n, int m, int out[] );

int main()
{
    int out[MAXN], n, m;
    int i;

    scanf("%d %d", &n, &m);
    CountOff( n, m, out );   
    for ( i = 0; i < n; i++ )
        printf("%d ", out[i]);
    printf("\n");

    return 0;
}
void CountOff( int n, int m, int out[] ){
   int i,j,p;
   int num[MAXN];
   for(i=0;i<n;i++)
   num[i]=i+1;
   int count=0;
   i=0;
   j=0;
   p=0;
   while(count<n){
   	if(num[i]!=-1){
    	p++;
   }
	if(p==m){
		j++;
		out[i]=j;
		p=0;
		num[i]=-1;
		count++;
	}
	i++;
	if(i==n)
	{
		i=0;
		
	 } 
}
}
