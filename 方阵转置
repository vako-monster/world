#include<stdio.h>
int main(){
	int n,i,j,c;
	scanf("%d",&n);
	int a[7][7];
	for(i=0;i<n;i++)
	for(j=0;j<n;j++)
	scanf("%d",&a[i][j]);
	//a[i][j]=i*n+1+j;}
	for(i=0;i<n;i++)
	   for(j=0;j<n;j++)
	      if(i<=j)
	{
		c=a[i][j];
		a[i][j]=a[j][i];
		a[j][i]=c;
		
	}
	for(i=0;i<n;i++){
		for(j=0;j<n;j++)
		printf("%4d",a[i][j]);
		printf("\n");
		 
	}
	return 0;}
