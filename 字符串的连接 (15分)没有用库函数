#include <stdio.h>
#include <string.h>

#define MAXS 10

char *str_cat( char *s, char *t );

int main()
{
    char *p;
    char str1[MAXS+MAXS] = {'\0'}, str2[MAXS] = {'\0'};

    scanf("%s%s", str1, str2);
    p = str_cat(str1, str2);
    printf("%s\n%s\n", p, str1);

    return 0;
}
char *str_cat(char *s,char *t){
	int i,count=0,p,j,cnt=0;
	for(i=0;t[i]!='\0';i++){
		count++;
	}
	for(j=0;s[j]!='\0';j++){
		cnt++;
	}

	for(p=0;t[p]!='\0';p++){
		s[cnt+p]=t[p];
	}
	s[cnt+p]='\0';
	return s;
} 
